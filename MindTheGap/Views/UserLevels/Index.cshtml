@model IEnumerable<MindTheGap.Models.UserLevel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.userLevel1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.xp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.xpNeeded)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AspNetUser.Email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.userLevel1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.xp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.xpNeeded)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AspNetUser.Email)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.levelId }) |
